@startuml
class Solution41{
    + main(String[])
    'nameSorter app = new nameSorter()
    'app.read()
    'app.sort()
    'app.output()
}

class nameSorter{
    'attributes
    - List<String> names
    - boolean fileRead = false

    'methods
    + nameSorter()

    + read(String): boolean
    + sort(): boolean
    + output(): void
}

Solution41 --o nameSorter
@enduml